{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 4005",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "generate": "graphql-codegen",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@reach/portal": "^0.11.2",
    "@tailwindcss/ui": "^0.6.2",
    "@vercel/fetch": "^6.1.1",
    "body-scroll-lock": "^3.1.5",
    "bowser": "^2.11.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "email-validator": "^2.0.4",
    "js-cookie": "^2.2.1",
    "keen-slider": "^5.5.1",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^10.2.3",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.4",
    "postcss-nesting": "^7.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.3.0",
    "react-icons": "^4.2.0",
    "react-merge-refs": "^1.1.0",
    "react-ticker": "^1.2.2",
    "swr": "^0.4.2",
    "tabbable": "^5.2.0",
    "tailwindcss": "^1.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@manifoldco/swagger-to-ts": "^2.1.0",
    "@next/bundle-analyzer": "^10.2.3",
    "@types/body-scroll-lock": "^2.6.2",
    "@types/bunyan": "^1.8.7",
    "@types/bunyan-prettystream": "^0.1.32",
    "@types/classnames": "^2.3.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.17.9",
    "@types/react": "^17.0.16",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "graphql": "^15.5.1",
    "next-unused": "^0.0.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "read-more-react": "^1.0.10",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "webpack": "5.11.1"
  },
  "license": "MIT"
}
